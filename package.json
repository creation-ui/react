{
  "name": "@creation-ui/react",
  "homepage": "https://creation-ui.com",
  "version": "0.1.7",
  "description": "@creation-ui/react is an easy-to-use components library for ReactJS & Tailwind CSS.",
  "repository": "https://github.com/creativetimofficial/creation-ui",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "swc ./src -d ./build --only ./src/**/*.ts --config-file .swcrc",
    "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
    "copy": "cp ./package.json ./build && cp ./README.md ./build && cp ./LICENSE ./build",
    "build": "yarn build:cjs && yarn build:dts && yarn copy && yarn build:version",
    "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "prettier:check": "prettier -c .",
    "prettify": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}\"",
    "_publish": "cd build && npm publish --registry https://registry.npmjs.org --access public",
    "build:version": "echo $(cat ./build/package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@headlessui/tailwindcss": "^0.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@tanstack/match-sorter-utils": "^8.7.2",
    "@tanstack/react-table": "^8.7.4",
    "@tanstack/table-core": "^8.7.4",
    "class-variance-authority": "^0.4.0",
    "clsx": "latest",
    "downshift": "^6.1.7",
    "react-datepicker": "^4.8.0",
    "react-spring": "^9.6.1",
    "react-stick": "^5.0.3",
    "tailwind-merge": "^1.5.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "tailwindcss": "^3.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.224",
    "@swc/plugin-styled-components": "^1.5.36",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.2",
    "styled-components": "^5.3.6",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "javascript",
    "react",
    "next",
    "create react app",
    "remix",
    "vite",
    "gatsby",
    "tailwind",
    "tailwind components",
    "tailwind elements",
    "tailwind library",
    "react library",
    "react components library",
    "tailwind sections",
    "tailwind css",
    "tailwind ui",
    "tailwind css react",
    "tailwind css next",
    "tailwind css remix",
    "tailwind css vite",
    "tailwind css gatsby"
  ]
}
