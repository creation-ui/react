{
  "name": "@creation-ui/react",
  "homepage": "https://creation-ui.com",
  "version": "0.1.19",
  "description": "@creation-ui/react is an easy-to-use components library for ReactJS & Tailwind CSS.",
  "repository": "https://github.com/creativetimofficial/creation-ui",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "copy": "cp ./package.json ./build && cp ./README.md ./build && cp ./LICENSE ./build  && cp ./src/index.css ./build",
    "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "prettier:check": "prettier -c .",
    "prettify": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}\"",
    "_publish": "cd build && npm publish --registry https://registry.npmjs.org --access public",
    "release": "yarn build && yarn _publish",
    "amend": "git commit --amend --no-edit",
    "build": "yarn build:rm && yarn build:cjs && yarn build:dts && yarn copy && yarn build:version",
    "build:cjs": "swc ./src -d ./build --only ./src/**/*.ts --config-file .swcrc",
    "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
    "build:version": "echo $(cat ./build/package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')",
    "build:rollup": "yarn build:version && yarn tsc && yarn build:rm && rollup -c",
    "build:rm": "rm -rf build",
    "ci:publish": "pnpm publish -r",
    "ci:version": "git fetch --tags && npm version $(git describe --abbrev=0 --tags)"

  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@headlessui/tailwindcss": "^0.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@tanstack/match-sorter-utils": "^8.7.6",
    "@tanstack/react-table": "^8.7.9",
    "@tanstack/table-core": "^8.7.9",
    "class-variance-authority": "^0.4.0",
    "clsx": "latest",
    "downshift": "^7.2.0",
    "react-datepicker": "^4.8.0",
    "react-spring": "^9.6.1",
    "react-stick": "^5.0.3",
    "tailwind-merge": "^1.5.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "tailwindcss": "^3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.28",
    "@swc/css": "^0.0.20",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.27",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "deepmerge": "^4.2.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rollup": "^3.10.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "javascript",
    "react",
    "next",
    "create react app",
    "remix",
    "vite",
    "gatsby",
    "tailwind",
    "tailwind components",
    "tailwind elements",
    "tailwind library",
    "react library",
    "react components library",
    "tailwind sections",
    "tailwind css",
    "tailwind ui",
    "tailwind css react",
    "tailwind css next",
    "tailwind css remix",
    "tailwind css vite",
    "tailwind css gatsby"
  ]
}
