{
  "name": "@creation-ui/react",
  "homepage": "https://creation-ui.com",
  "version": "0.10.3",
  "description": "@creation-ui/react is an easy-to-use components library for ReactJS & Tailwind CSS.",
  "repository": "https://github.com/creativetimofficial/creation-ui",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "copy": "cp ./package.json ./build && cp ./README.md ./build && cp ./LICENSE ./build  && cp ./src/index.css ./build",
    "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "prettier:check": "prettier -c .",
    "prettify": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}\"",
    "_publish": "cd build && npm publish --registry https://registry.npmjs.org --access public",
    "release": "pnpm build && pnpm _publish",
    "amend": "git commit --amend --no-edit",
    "build": "pnpm build:rm && pnpm build:cjs && pnpm build:dts && pnpm copy && pnpm build:version",
    "build:cjs": "swc ./src -d ./build --only ./src/**/*.ts --config-file .swcrc",
    "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
    "build:version": "echo $(cat ./build/package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')",
    "build:rollup": "pnpm build:version && pnpm tsc && pnpm build:rm && rollup -c",
    "build:rm": "rm -rf build",
    "ci:publish": "pnpm publish -r",
    "ci:version": "git fetch --tags && npm version $(git describe --abbrev=0 --tags)"
  },
  "dependencies": {
    "@floating-ui/react": "^0.25.1",
    "@floating-ui/react-dom": "^2.0.1",
    "@headlessui/react": "^1.7.13",
    "@headlessui/tailwindcss": "^0.1.3",
    "@tanstack/match-sorter-utils": "^8.7.6",
    "@tanstack/react-table": "^8.7.9",
    "@tanstack/table-core": "^8.7.9",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "keyboard-key": "^1.1.0",
    "lodash.merge": "^4.6.2",
    "lodash.values": "^4.3.0",
    "mini-svg-data-uri": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-spring": "^9.7.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "tailwindcss": "^3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@swc/css": "^0.0.20",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.values": "^4.3.7",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "rollup": "^3.20.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "javascript",
    "react",
    "next",
    "create react app",
    "remix",
    "vite",
    "gatsby",
    "tailwind",
    "tailwind components",
    "tailwind elements",
    "tailwind library",
    "react library",
    "react components library",
    "tailwind sections",
    "tailwind css",
    "tailwind ui",
    "tailwind css react",
    "tailwind css next",
    "tailwind css remix",
    "tailwind css vite",
    "tailwind css gatsby"
  ]
}
